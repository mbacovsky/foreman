<%#
name: Host statuses CSV
snippet: false
model: ReportTemplate
-%>
<%- load_hosts(includes: :host_statuses).each_record do |host| -%>
<%-   report_row({
        'Name': host.name,
        'Global': host.global_status
      }.merge(all_host_statuses_hash(host))) -%>
<%- end -%>
<%= report_render -%>
